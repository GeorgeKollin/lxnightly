pywexit="$?";
if [ "$pywexit" -ne "0" ] && [ "$isxterm" -eq "0" ];
    then xterm --noclose -e "echo -e 'Python 3.6.8 is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3\n'";
    exit 1;
