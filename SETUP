    then gnome-terminal --noclose -e "echo -e 'Python 3 module tkinter is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3-tk\n'";
    exit 1;
elif [ "$tkval" -ne "0" ] && [ "$iskonsole" -eq "0" ];
    then konsole --noclose -e "echo -e 'Python 3 module tkinter is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3-tk\n'";
    exit 1;
elif [ "$tkval" -ne "0" ] && [ "$islxterminal" -eq "0" ];
    then lxterminal --noclose -e "echo -e 'Python 3 module tkinter is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3-tk\n'";
    exit 1;
elif [ "$tkval" -ne "0" ] && [ "$isxfceterm" -eq "0" ];
    then xfce4-terminal --noclose -e "echo -e 'Python 3 module tkinter is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3-tk\n'";
    exit 1;
elif [ "$tkval" -ne "0" ];
    then echo -e "Python 3 module tkinter is required in order to run \"SETUP\".\nPlease, run the command below to install Python 3:\n\tsudo apt-get install python3-tk\n";
    exit 1;
fi;

