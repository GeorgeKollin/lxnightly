    then xterm --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$isterminator" -eq "0" ];
    then terminator --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$isrxvt" -eq "0" ];
    then rxvt --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$isgnometerm" -eq "0" ];
    then gnome-terminal --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$iskonsole" -eq "0" ];
    then konsole --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$islxterminal" -eq "0" ];
    then lxterminal --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ] && [ "$isxfceterm" -eq "0" ];
    then xfce4-terminal --noclose -e "echo -e 'Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$sysval" -ne "0" ];
    then echo -e "Python 3 module sys is missing.\nUnfortunately, it's impossible to run this program on your system.\n";
    exit 1;
fi;

if [ "$subval" -ne "0" ] && [ "$isxterm" -eq "0" ];
    then xterm --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$isterminator" -eq "0" ];
    then terminator --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$isrxvt" -eq "0" ];
    then rxvt --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$isgnometerm" -eq "0" ];
    then gnome-terminal --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$iskonsole" -eq "0" ];
    then konsole --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$islxterminal" -eq "0" ];
    then lxterminal --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ] && [ "$isxfceterm" -eq "0" ];
    then xfce4-terminal --noclose -e "echo -e 'Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$subval" -ne "0" ];
    then echo -e "Python 3 module subprocess is missing.\nUnfortunately, it's impossible to run this program on your system.\n";
    exit 1;
fi;

if [ "$osval" -ne "0" ] && [ "$isxterm" -eq "0" ];
    then xterm --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$isterminator" -eq "0" ];
    then terminator --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$isrxvt" -eq "0" ];
    then rxvt --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$isgnometerm" -eq "0" ];
    then gnome-terminal --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$iskonsole" -eq "0" ];
    then konsole --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$islxterminal" -eq "0" ];
    then lxterminal --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ] && [ "$isxfceterm" -eq "0" ];
    then xfce4-terminal --noclose -e "echo -e 'Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n'";
    exit 1;
elif [ "$osval" -ne "0" ];
    then echo -e "Python 3 module os is missing.\nUnfortunately, it's impossible to run this program on your system.\n";
    exit 1;
fi;

python3 "./gui/frstwin.py";

if [ "$?" -ne "0" ];
    then exit 1;
fi;

python3 "./gui/initwin.py" &
waittsk="$!";

touch "./src/SETUP_err";
echo "" > "./src/SETUP_err";

mkdir "./conf";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/randr_en";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_randr_en" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/no_fork";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_no_fork" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/zip_all";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_zip_all" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/lat_deg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_lat_deg" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/zip_code";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_zip_code" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/exec_out.txt";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_exec_out" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/lgtd_deg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_lgtd_deg" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/tmprt_indx";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_tmprt_indx" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/root_path";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_root_path" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/thm_cfg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_thm_cfg" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/flux_exit.txt";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_flux_exit" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
touch "./conf/start.sh";
echo "0" > "./conf/zip_all";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_zipallval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "0" > "./conf/no_fork";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_noforkval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "0" > "./conf/randr_en";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_randrval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "1" > "./conf/lat_deg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_latval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "0" > "./conf/zip_code";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_zipval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "d" > "./conf/thm_cfg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_thmval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "1" > "./conf/lgtd_deg";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_lgtdval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo "3400" > "./conf/tmprt_indx";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_tmprtval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
cdir=$(pwd);
echo "$cdir" > "./conf/root_path";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_pathval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
echo -e "#!/bin/bash\ncd '$cdir';\npython3 './gui/lxnightcfg.py';" > "./conf/start.sh";
chmod +x "./conf/start.sh";
if [ "$?" -ne "0" ];
    then echo "Error: err_conf_exitval" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;

kill -9 "$waittsk";
python3 "./gui/thememngr.py";
python3 "./gui/waitwin.py" &
waittsk="$!";

mkdir "./bin";
if [ "$?" -ne "0" ];
    then echo "Error: err_bin" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;
if ! [ -f "./bin/xflux64.tgz" ];
    then wget "https://justgetflux.com/linux/xflux64.tgz";
    if [ "$?" -ne "0" ];
        then echo "Error: err_internet_disconnected" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
    mv "./xflux64.tgz" "./bin";
    if [ "$?" -ne "0" ];
        then echo "Error: err_binmv_64" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
fi;
if ! [ -f "./bin/xflux-pre.tgz" ];
    then wget "https://justgetflux.com/linux/xflux-pre.tgz";
    if [ "$?" -ne "0" ];
        then echo "Error: err_internet_disconnected" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
    mv "./xflux-pre.tgz" "./bin";
    if [ "$?" -ne "0" ];
        then echo "Error: err_binmv_32" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
fi;
if ! [ -f "./bin/xflux.tgz" ];
    then wget "https://justgetflux.com/linux/xflux.tgz";
    if [ "$?" -ne "0" ];
        then echo "Error: err_internet_disconnected" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
    mv "./xflux.tgz" "./bin";
    if [ "$?" -ne "0" ];
        then echo "Error: err_binmv_old" >> "./src/SETUP_err";
        kill -9 "$waittsk";
        python3 "./gui/insterrwin.py";
        exit 1;
    fi;
fi;

sysarch=$(uname -m);

case "$sysarch" in
    *"i386"*)
        tar -zxf "./bin/xflux-pre.tgz";
        if [ "$?" -ne "0" ];
            then echo "Error: err_extr_32" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
        mv "./xflux" "./bin";
        if [ "$?" -ne "0" ];
            then echo "Error: err_binmv_32_extr" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
    ;;
    *"i686"*)
        tar -zxf "./bin/xflux-pre.tgz";
        if [ "$?" -ne "0" ];
            then echo "Error: err_extr_32" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
        mv "./xflux" "./bin";
        if [ "$?" -ne "0" ];
            then echo "Error: err_binmv_32_extr" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
    ;;
    *)
        tar -zxf "./bin/xflux64.tgz";
        if [ "$?" -ne "0" ];
            then echo "Error: err_extr_64" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
        mv "./xflux" "./bin";
        if [ "$?" -ne "0" ];
            then echo "Error: err_binmv_64_extr" >> "./src/SETUP_err";
            kill -9 "$waittsk";
            python3 "./gui/insterrwin.py";
            exit 1;
        fi;
    ;;
esac;

chmod +x "./src/"*;
if [ "$?" -ne "0" ];
    then echo "Error: err_chmod" >> "./src/SETUP_err";
    kill -9 "$waittsk";
    python3 "./gui/insterrwin.py";
    exit 1;
fi;

thmres=$(< "./conf/thm_cfg");
case "$thmres" in
    *"l"*)
        "./src/light.sh";
    ;;
    *)
        "./src/dark.sh";
    ;;
esac;

mv "./src/unins.old" "./UNINSTALL";
mv "./SETUP" "./src/setup.old";
mkdir "./backup";

kill -9 "$waittsk";
python3 "./gui/finwin.py";

exit 0;
